# Node.js dependencies
*/node_modules

# Build and output directories
frontend/build/
frontend/dist/
backend/build/
backend/dist/

# Environment variable files
frontend/.env
backend/.env

# Logs
*.log
frontend/logs/
backend/logs/
npm-debug.log*
yarn-debug.log*
yarn-error.log*

# OS-specific files
.DS_Store
Thumbs.db

# IDE-specific files
.vscode/
.idea/
*.suo
*.ntvs*
*.njsproj
*.sln

# Ignore compiled code or temporary files
*.swp
*.bak
*.tmp
*.temp

# Other generated files (e.g., coverage reports)
coverage/
frontend/coverage/
backend/coverage/

# Enforce LF endings for specific file types
*.js text eol=lf
*.jsx text eol=lf
*.json text eol=lf
*.md text eol=lf
*.css text eol=lf
*.html text eol=lf
*.config text eol=lf
